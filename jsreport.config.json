{
  "httpPort": 5488,
  "store": {
    "provider": "fs"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "authentication": {
    "enabled": true,
    "admin": {
      "username": "admin",
      "password": "admin123"
    },
    "cookieSession": {
      "secret": "your-secret-key-2024"
    }
  },
  "extensions": {
    "chrome-pdf": {
      "enabled": true,
      "launchOptions": {
        "headless": "new",
        "args": [
          "--no-sandbox",
          "--disable-setuid-sandbox",
          "--disable-dev-shm-usage",
          "--disable-accelerated-2d-canvas",
          "--no-first-run",
          "--no-zygote",
          "--single-process",
          "--disable-gpu"
        ]
      },
      "timeout": 60000
    },
    "fs-store": {
      "enabled": true
    },
    "authentication": {
      "enabled": true
    }
  },
  "workers": {
    "numberOfWorkers": 1,
    "timeout": 60000
  },
  "trustUserCode": false,
  "allowLocalFilesAccess": false,
  "logger": {
    "console": {
      "transport": "console",
      "level": "info"
    }
  }
}