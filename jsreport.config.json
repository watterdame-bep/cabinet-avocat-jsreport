{
  "store": {
    "provider": "fs"
  },
  "chrome": {
    "launchOptions": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-dev-shm-usage",
        "--disable-gpu",
        "--no-first-run"
      ]
    }
  },
  "tempDirectory": "/app/tmp",
  "httpPort": 5488,
  "authentication": {
    "cookieSession": {
      "secret": "${JSREPORT_COOKIE_SECRET}"
    },
    "admin": {
      "username": "${JSREPORT_ADMIN_USER}",
      "password": "${JSREPORT_ADMIN_PASSWORD}"
    }
  },
  "extensions": {
    "authentication": {
      "enabled": true
    }
  }
}
